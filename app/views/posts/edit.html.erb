<%= form_for @post do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, :size => 140 %> 

    <%= f.label :image %>
    <%= f.file_field :image %>

    <%= f.label :image2 %>
    <%= f.file_field :image2 %>

    <%= f.label :image3 %>
    <%= f.file_field :image3 %>
      
    <%= f.label :image4 %>
    <%= f.file_field :image4 %>

    <%= f.label :detail %>
    <%= f.text_field :detail, :size => 140 %> 
    <%= f.submit "編集する" %>
  <% end %>